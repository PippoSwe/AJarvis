{{#items}}
       <div id="list-item-{{id}}" class="list-group-item pb-5">
           <h5 class="list-group-item-heading float-left pt-2">{{project}}</h5>
           <div class="btn-group pull-right">
               <button type="button" data-id="{{id}}" class="edit btn btn-secondary"><span class="ion-edit pr-2 rm-padding-icon"></span><span class="hidden-text">Modifica</span></button>
               <button type="button" data-id="{{id}}" class="delete btn btn-danger" data-toggle="modal" data-target="#exampleModalCenter"><span class="ion-ios-trash pr-2 rm-padding-icon"></span><span class="hidden-text">Cancella</span></button>
           </div>
       </div>
{{/items}}